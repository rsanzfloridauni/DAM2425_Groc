<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="main">
	<class name="Imagen" table="image">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="imageName" type="string" column="imageName" />
		<property name="theme" type="string" column="theme" />
		<property name="difficulty" type="int" column="difficulty" />
		<property name="link" type="string" column="link" />
	</class>
</hibernate-mapping>